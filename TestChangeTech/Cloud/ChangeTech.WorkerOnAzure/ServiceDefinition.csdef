<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="ChangeTech.WorkerOnAzure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WorkerRole name="ChangeTech.Worker">
    <ConfigurationSettings>
      <Setting name="DiagnosticsConnectionString" />
      <Setting name="ManualyEmailDateTime" />
    </ConfigurationSettings>
    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
      <Import moduleName="Diagnostics" />
    </Imports>
  </WorkerRole>
</ServiceDefinition>